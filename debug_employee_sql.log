SQL Statement:

        MERGE dbo.Employees AS t
        USING (SELECT ? AS ClientId, ? AS Name, ? AS BirthDate) AS s
        ON (t.ClientId=s.ClientId AND t.Name=s.Name AND t.BirthDate=s.BirthDate)
        WHEN MATCHED THEN
            UPDATE SET EmploymentType=?, SalaryType=?, BaseSalary=?, HourlyRate=?, NormalHours=?, FoodAllowance=?, CarAllowance=?, EmailTo=?, EmailCc=?, UseEmail=?, HasNationalPension=?, HasHealthInsurance=?, HasEmploymentInsurance=?, HealthInsuranceBasis=?, PensionInsurableWage=?, JoinDate=?, ResignDate=?, TaxDependents=?, ChildrenCount=?, TaxFreeMeal=?, TaxFreeCarMaintenance=?, OtherTaxFree=?, IncomeTaxRate=?, UpdatedAt=SYSUTCDATETIME()
        WHEN NOT MATCHED THEN
            INSERT (ClientId, Name, BirthDate, EmploymentType, SalaryType, BaseSalary, HourlyRate, NormalHours, FoodAllowance, CarAllowance, EmailTo, EmailCc, UseEmail, HasNationalPension, HasHealthInsurance, HasEmploymentInsurance, HealthInsuranceBasis, PensionInsurableWage, JoinDate, ResignDate, TaxDependents, ChildrenCount, TaxFreeMeal, TaxFreeCarMaintenance, OtherTaxFree, IncomeTaxRate)
            VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?);
        

